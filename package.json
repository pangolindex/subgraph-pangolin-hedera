{
  "name": "subgraph-pangolin",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare:testnet": "mustache config/testnet.json subgraph.template.yaml > subgraph.yaml && mustache config/testnet.json src/mappings/helpers.template.ts > src/mappings/helpers.ts && mustache config/testnet.json src/mappings/pricing.template.ts > src/mappings/pricing.ts",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml && mustache config/mainnet.json src/mappings/helpers.template.ts > src/mappings/helpers.ts && mustache config/mainnet.json src/mappings/pricing.template.ts > src/mappings/pricing.ts",
    "codegen:testnet": "mustache config/testnet.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "codegen:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "build:testnet": "yarn prepare:testnet && graph build",
    "build:mainnet": "yarn prepare:mainnet && graph build",
    "create:local": "graph create --node http://localhost:8020 local",
    "create:testnet": "graph create --node https://hedera-test.pangolin.network:8020 pangolin",
    "create:testnet-staging": "graph create --node https://hedera-test.pangolin.network:8020 pangolin-staging",
    "create:mainnet": "graph create --node https://hedera-graph.pangolin.network:8020 pangolin",
    "create:mainnet-staging": "graph create --node https://hedera-graph.pangolin.network:8020 pangolin-staging",
    "remove:local": "graph remove --node http://localhost:8020 local",
    "remove:testnet": "graph remove --node https://hedera-test.pangolin.network:8020 pangolin",
    "remove:testnet-staging": "graph remove --node https://hedera-test.pangolin.network:8020 pangolin-staging",
    "remove:mainnet": "graph remove --node https://hedera-graph.pangolin.network:8020 pangolin",
    "remove:mainnet-staging": "graph remove --node https://hedera-graph.pangolin.network:8020 pangolin-staging",
    "deploy:local": "graph deploy --node http://localhost:8020 --ipfs http://localhost:5001 local",
    "deploy:testnet": "graph deploy --node https://hedera-test.pangolin.network:8020 --ipfs http://hedera-test.pangolin.network:5001 pangolin",
    "deploy:testnet-staging": "graph deploy --node https://hedera-test.pangolin.network:8020 --ipfs http://hedera-test.pangolin.network:5001 pangolin-staging",
    "deploy:mainnet": "graph deploy --node https://hedera-graph.pangolin.network:8020 --ipfs http://hedera-graph.pangolin.network:5001 pangolin",
    "deploy:mainnet-staging": "graph deploy --node https://hedera-graph.pangolin.network:8020 --ipfs http://hedera-graph.pangolin.network:5001 pangolin-staging"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.44.0",
    "@graphprotocol/graph-ts": "0.29.3",
    "mustache": "4.2.0"
  }
}
